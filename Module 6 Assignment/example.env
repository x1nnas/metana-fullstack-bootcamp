# Settings for the Express API backend
# Copy these to .env in the project root


# The port on which the Express server will run
PORT=3000

# PostgreSQL connection string
# Format: postgresql://username:password@hostname:port/database

DATABASE_URL=postgresql://username:password@hostname:port/database

# Additional PostgreSQL settings (if applicable)

PGHOST=hostname
PGUSER=username
PGPASSWORD=password
PGDATABASE=database
PGPORT=5432